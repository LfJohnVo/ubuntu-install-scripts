#!/usr/bin/env bash

# Finds the scripts here which are not listed in any of the `all-*` scripts.

set -e 
cd $(dirname ${0})

for section in sys user; do (cd $section && for i in $(find . -maxdepth 1 -type f -not -name all-\*); do (grep $i all-* > /dev/null || (echo $section/$i | sed -r 's/\/\.\//\//')); done;); done;
